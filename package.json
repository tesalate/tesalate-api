{
  "name": "tesalate-api",
  "version": "0.1.0",
  "description": "API for tesalate",
  "main": "src/index.js",
  "repository": "https://github.com/tesalate/main-api.git",
  "author": "Devin Cunningham",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "commit": "cz",
    "release": "standard-version",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.html dist/",
    "build": "yarn clean && tsc && yarn copy-files",
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "NODE_ENV=development nodemon src/index.ts",
    "test": "jest --colors --verbose --detectOpenHandles",
    "test:coverage": "jest --silent --testLocationInResults --ci --all --coverage --json --outputFile=\"report.json\"",
    "test:watch": "jest -i --watchAll --verbose",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "node",
    "node.js",
    "express",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier",
    "typescript"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-oas-generator": "^1.0.41",
    "express-rate-limit": "^5.0.0",
    "express-swagger-generator": "^1.1.17",
    "express-ws": "^5.0.2",
    "geolib": "^3.3.3",
    "global": "^4.4.0",
    "handlebars": "^4.7.7",
    "helmet": "^4.1.0",
    "http-status": "^1.4.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.7.7",
    "mongoose-autopopulate": "^0.16.0",
    "mongoose-to-swagger": "^1.4.0",
    "morgan": "^1.9.1",
    "nodemailer": "^6.3.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pm2": "^5.1.0",
    "query-string": "^7.0.1",
    "set-cookie-parser": "^2.4.8",
    "swagger-jsdoc": "^6.0.8",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.4.0",
    "tsc": "^2.0.4",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "winston": "^3.2.1",
    "ws": "^8.4.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie": "^0.4.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^7.29.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/express-ws": "^3.0.1",
    "@types/faker": "^5.5.9",
    "@types/is-number": "^7.0.2",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.178",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.10",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemon": "^1.19.1",
    "@types/passport": "^0.4.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/prettier": "^2.4.3",
    "@types/set-cookie-parser": "^2.4.2",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.1",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.0.7",
    "cz-conventional-changelog": "3.3.0",
    "electron": "^16.0.5",
    "eslint": "8.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^5.1.0",
    "file-system": "^2.2.2",
    "husky": "^7.0.1",
    "is-number": "^7.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^11.0.0",
    "mongodb-memory-server": "^8.2.0",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^2.0.0",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "readline": "^1.3.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "supertest": "^6.0.1",
    "ts-jest": "^27.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
